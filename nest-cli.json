{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/erp/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/erp/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/erp",
  "projects": {
    "erp": {
      "type": "application",
      "root": "apps/erp",
      "entryFile": "main",
      "sourceRoot": "apps/erp/src",
      "compilerOptions": {
        "tsConfigPath": "apps/erp/tsconfig.app.json"
      }
    },
    "common": {
      "type": "application",
      "root": "apps/common",
      "entryFile": "main",
      "sourceRoot": "apps/common/src",
      "compilerOptions": {
        "tsConfigPath": "apps/common/tsconfig.app.json"
      }
    },
    "inventory": {
      "type": "application",
      "root": "apps/inventory",
      "entryFile": "main",
      "sourceRoot": "apps/inventory/src",
      "compilerOptions": {
        "tsConfigPath": "apps/inventory/tsconfig.app.json"
      }
    },
    "crm": {
      "type": "application",
      "root": "apps/crm",
      "entryFile": "main",
      "sourceRoot": "apps/crm/src",
      "compilerOptions": {
        "tsConfigPath": "apps/crm/tsconfig.app.json"
      }
    },
    "sales": {
      "type": "application",
      "root": "apps/sales",
      "entryFile": "main",
      "sourceRoot": "apps/sales/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sales/tsconfig.app.json"
      }
    }
  }
}